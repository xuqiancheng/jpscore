<?xml version="1.0" encoding="UTF-8" ?>

<!-- <JuPedSim project="JPS-Project" version="0.8" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ini.xsd"> -->
<JuPedSim project="JPS-Project" version="0.8" xmlns:xsi="http://xsd.jupedsim.org/jps_ini_core.xsd" xsi:noNamespaceSchemaLocation="ini.xsd">
  <seed>linspace(1,10000,4)</seed>
  <!-- <seed>1</seed> -->
  <num_threads>8</num_threads>
  <max_sim_time unit="sec">100</max_sim_time>
  <!-- geometry file -->
  <geometry>Validation/test_13/geometries/</geometry>
  <!-- traectories file and format -->
  <trajectories format="xml-plain" embed_mesh="false" fps="20">
    <file location="Traj_test_13.xml" />
  </trajectories>
  <!-- where to store the logs -->
  <!--logfile>log</logfile-->

  <!-- traffic information: e.g closed doors or smoked rooms -->
  <traffic_constraints>
    <!-- room states are: good or smoked -->
    <rooms>
      <room room_id="0" state="good" />
    </rooms>
    
    <!-- doors states are: close or open -->
    <doors>
    </doors>

  </traffic_constraints>
  
  <routing>
    <goals>
      <!-- <goal id="0" final="true" caption="goal 0"> -->
      <!--   <polygon> -->
      <!--     <vertex px="11.0" py="2.0" /> -->
      <!--     <vertex px="11.0" py="3.0" /> -->
      <!--     <vertex px="12.0" py="3.0" /> -->
      <!--     <vertex px="12.0" py="2.0" /> -->
      <!--     <vertex px="11.0" py="2.0" /> -->
      <!--   </polygon> -->
      <!-- </goal> -->
    </goals>
  </routing>

  
  <!--persons information and distribution -->
  <agents operational_model_id="6">
    <agents_distribution>
      <group group_id="0"  agent_parameter_id="1" room_id="0" subroom_id="2" number="100" goal_id="-1" router_id="1"/>	
	   <!--group group_id="1"  agent_parameter_id="1" room_id="0" subroom_id="2" number="50" goal_id="-1" router_id="1"/-->
    </agents_distribution>

    <!-- frequency in persons/minute -->
    <agents_sources/>
  </agents>
 
  <operational_models >
  <model operational_model_id="6" description="gcvm">
      <model_parameters>
        <solver>euler</solver>
        <stepsize>0.05</stepsize>
        <exit_crossing_strategy>3</exit_crossing_strategy>
        <linkedcells enabled="true" cell_size="300" />
        <force_ped a="3" D="0.1" />
        <force_wall a="6" D="0.05" />
		<time_parameters Ts="0.45" Td="0.3" />
      </model_parameters>
      <agent_parameters agent_parameter_id="1">
        <v0 mu="1.34" sigma="0.26" />
        <bmax mu="0.20" sigma="0.00000" /> <!-- this is l, assuming peds are circles with constant radius -->
        <bmin mu="0.20" sigma="0.00000" />
        <amin mu="0.20" sigma="0.00000" />
        <atau mu="0.0" sigma="0.00000" />
		<tau mu="0.5" sigma="0.000" />
		<shape circle="1" /> <!--circle=1:using circle, circle=0:using ellipse-->
      </agent_parameters>
	  <agent_parameters agent_parameter_id="2">
        <v0 mu="1.34" sigma="0.26" />
        <bmax mu="0.25" sigma="0.00000" /> <!-- this is l, assuming peds are circles with constant radius -->
        <bmin mu="0.15" sigma="0.00000" />
        <amin mu="0.18" sigma="0.00000" />
        <atau mu="0.00" sigma="0.00000" />
		<tau mu="0.5" sigma="0.000" />
		<shape circle="0" /> <!--circle=1:using circle, circle=0:using ellipse-->
      </agent_parameters>
    </model>
	
      <model operational_model_id="3" description="Tordeux2015">
      <model_parameters>
        <solver>euler</solver>
        <stepsize>0.05</stepsize>
        <exit_crossing_strategy>3</exit_crossing_strategy>
        <linkedcells enabled="true" cell_size="3" />
        <force_ped  a="3.0" D="0.1"/>
        <force_wall a="6.0" D="0.05"/>
      </model_parameters>
      <agent_parameters agent_parameter_id="0">
        <v0 mu="1.34" sigma="0.26" />
        <bmax mu="0.25" sigma="0.0" /> <!-- this is l, assuming peds are circles with constant radius-->
        <bmin mu="0.20" sigma="0.0" />
        <amin mu="0.18" sigma="0.0" />
        <atau mu="0.1" sigma="0.0" />
		<tau mu="0.3" sigma="0.00" />
        <T mu="0.5" sigma="0.0"/>
		<alpha mu="1.0" sigma="0.0"/>
      </agent_parameters>
	  <agent_parameters agent_parameter_id="1">
        <v0 mu="1.34" sigma="0.26" />
        <bmax mu="0.25" sigma="0.0" /> <!-- this is l, assuming peds are circles with constant radius-->
        <bmin mu="0.25" sigma="0.0" />
        <amin mu="0.25" sigma="0.0" />
        <tau mu="0.3" sigma="0.00" />
        <atau mu="0.0" sigma="0.0" />
        <T mu="0.55" sigma="0.00"/>
		<alpha mu="1.0" sigma="0.0"/>
      </agent_parameters>
	  <agent_parameters agent_parameter_id="2">
        <v0 mu="1.34" sigma="0.26" />
        <bmax mu="0.18" sigma="0.0" /> <!-- this is l, assuming peds are circles with constant radius-->
        <bmin mu="0.18" sigma="0.0" />
        <amin mu="0.18" sigma="0.0" />
        <tau mu="0.3" sigma="0.00" />
        <atau mu="0.0" sigma="0.0" />
        <T mu="0.40" sigma="0.00"/>
		<alpha mu="1.0" sigma="0.0"/>
      </agent_parameters>
	  <agent_parameters agent_parameter_id="3">
        <v0 mu="1.34" sigma="0.26" />
        <bmax mu="0.20" sigma="0.0" /> <!-- this is l, assuming peds are circles with constant radius-->
        <bmin mu="0.15" sigma="0.0" />
        <amin mu="0.13" sigma="0.0" />
        <tau mu="0.3" sigma="0.00" />
        <atau mu="0.1" sigma="0.0" />
        <T mu="0.45" sigma="0.00"/>
		<alpha mu="1.0" sigma="0.0"/>
      </agent_parameters>
    </model>

    <model operational_model_id="1" description="gcfm">
      <model_parameters>
        <solver>euler</solver>
        <stepsize>0.001</stepsize>
        <exit_crossing_strategy>2</exit_crossing_strategy>
        <linkedcells enabled="true" cell_size="2.2" />
        <force_ped nu="0.2" dist_max="2" disteff_max="2" interpolation_width="0.1" />
        <force_wall nu="0.2" dist_max="1" disteff_max="1" interpolation_width="0.1" />
      </model_parameters>
      <agent_parameters agent_parameter_id="0">
        <v0 mu="1.34" sigma="0.26" />
        <bmax mu="0.25" sigma="0.001" />
        <bmin mu="0.20" sigma="0.001" />
        <amin mu="0.18" sigma="0.001" />
        <tau mu="0.5" sigma="0.001" />
        <atau mu="0.43" sigma="0.001" />
      </agent_parameters>
    </model>

    <model operational_model_id="5" description="krausz">
      <model_parameters>
        <solver>euler</solver>
        <stepsize>0.001</stepsize>
        <exit_crossing_strategy>2</exit_crossing_strategy>
        <linkedcells enabled="true" cell_size="2.2" />
        <force_ped nu="0.2" dist_max="2" disteff_max="2" interpolation_width="0.1" />
        <force_wall nu="0.2" dist_max="1" disteff_max="1" interpolation_width="0.1" />
      </model_parameters>
      <agent_parameters agent_parameter_id="0">
        <v0 mu="1.34" sigma="0.26" />
        <bmax mu="0.25" sigma="0.001" />
        <bmin mu="0.20" sigma="0.001" />
        <amin mu="0.18" sigma="0.001" />
        <tau mu="0.5" sigma="0.001" />
        <atau mu="0.43" sigma="0.001" />
      </agent_parameters>
    </model>

    <model operational_model_id="2" description="gompertz">
      <model_parameters>
        <solver>euler</solver>
        <stepsize>0.001</stepsize>
        <exit_crossing_strategy>3</exit_crossing_strategy>
        <linkedcells enabled="true" cell_size="3" />
        <!-- <force_ped nu="0.5"  b="0.06" c="3.45"/> -->
        <!-- <force_wall nu="5" b="0.4" c="10.0"/> -->
        <force_ped  nu="0."  b="0.034229" c="2.450932"/>
        <force_wall nu="0."  b="0.034229" c="2.450932"/>

      </model_parameters>
      <agent_parameters agent_parameter_id="0">
        <v0 mu="1.34" sigma="0.0" />
        <bmax mu="0.25" sigma="0.001" />
        <bmin mu="0.40" sigma="0.001" />
        <amin mu="0.18" sigma="0.001" />
        <tau mu="0.5" sigma="0.001" />
        <atau mu="0.23" sigma="0.001" />
      </agent_parameters>
    </model>
  </operational_models>

  <route_choice_models>
    <router router_id="1" description="local_shortest">
      <parameters>
        <!--<navigation_lines file="routing.xml" />-->
      </parameters>
    </router>
  </route_choice_models>

</JuPedSim>
